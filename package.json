    "eslint": "^6.4.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lerna": "^3.16.4",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  },
  "husky": {
    "hooks": {
