{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 3%, not dead"
      }
    ],
    "@babel/preset-react",
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": [
    [
      "babel-plugin-style-props",
      {
        "stylingLibrary": "emotion",
        "variants": {
          "boxStyle": "boxStyles"
        }
      }
    ]
  ]
}
