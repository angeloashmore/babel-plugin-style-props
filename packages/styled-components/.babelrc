{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 3%, not dead"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-style-props",
      {
        "stylingLibrary": "styled-components",
        "variants": {
          "boxStyle": "boxStyles"
        }
      }
    ],
    "babel-plugin-styled-components"
  ]
}
