{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 3%, not dead"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@styled-system/babel-plugin",
      {
        "stylingLibrary": "styled-components",
        "variants": {
          "boxStyle": "boxStyles"
        }
      }
    ],
    "babel-plugin-styled-components"
  ]
}
